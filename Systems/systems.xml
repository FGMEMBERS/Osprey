<PropertyList>

    <electrical>
        <name>electrical</name>
        <number>0</number>
        <path>Aircraft/Generic/generic-electrical.xml</path>
    </electrical>

    <pitot>
        <name>pitot</name>
        <number>0</number>
    </pitot>

    <static>
        <name>static</name>
        <number>0</number>
        <tau>1</tau>
    </static>

    <vacuum>
        <name>vacuum</name>
        <number>0</number>

        <!-- Right engine -->
        <rpm>/rotors/main/rpm</rpm>

        <scale>10.0</scale>
    </vacuum>

    <vacuum>
        <name>vacuum</name>
        <number>1</number>

        <!-- Left engine -->
        <rpm>/rotors/tail/rpm</rpm>

        <scale>10.0</scale>
    </vacuum>

</PropertyList>
